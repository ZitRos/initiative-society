{
  "name": "initiative-society",
  "version": "0.9.0",
  "description": "A prototype of online social welfare platform on Ethereum Blockchain.",
  "main": "index.js",
  "dependencies": {
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "graphql": "^0.12.3",
    "npm-run-all": "^4.1.2",
    "sha1": "^1.1.1",
    "truffle": "^4.0.3"
  },
  "devDependencies": {},
  "scripts": {
    "init": "npm install && cd src/client && npm install",
    "start": "cd src/client && npm run start",
    "build": "cd src/client && npm run build",
    "test": "npm run truffle -- test",
    "truffle": "cd src/ethereum && truffle",
    "testnet": "npm run truffle -- develop",
    "win-test": "npm run win-truffle -- test",
    "win-truffle": "cd src/ethereum && truffle.cmd",
    "win-testnet": "npm run win-truffle -- develop",
    "pull": "git pull --recurse-submodules && git submodule update",
    "server": "node src/server/index.js",
    "backend": "npm-run-all --parallel server start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ZitRos/initiative-society.git"
  },
  "keywords": [
    "initiative",
    "society",
    "social",
    "welfare",
    "blockchain",
    "ethereum",
    "online",
    "platform"
  ],
  "author": "Nikita & Natalie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ZitRos/initiative-society/issues"
  },
  "homepage": "https://github.com/ZitRos/initiative-society#readme"
}
